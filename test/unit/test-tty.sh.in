#!@SH_PROG@
# -*- shell-script -*-
PS4='(%x:%I): [%?] zsh+ 
'
setopt ksharrays

test_tty()
{
    typeset -i _Dbg_new_fd
    typeset -a _Dbg_fd
    _Dbg_fd[0]=1
    _Dbg_open_if_tty $0
    assertEquals "1" "$?"
    assertEquals "-1" "$_Dbg_new_fd"
    tty=$(tty)
    if (($? == 0)); then 
	if [[ $tty =~ 'not a tty' ]] ; then
	    result=1
	    :
	else
	    result=0
	fi
	_Dbg_open_if_tty $tty
	assertEquals "$result" "$?"
	if ((result == 0)) ; then
	    assertNotEquals "-1" "$Dbg_new_fd"
	    exec {_Dbg_new_fd}>&- 
	    _Dbg_new_fd=-1
	    _Dbg_set_tty $tty
	    assertEquals "$result" "$?"
	    assertNotEquals "-1" "$Dbg_new_fd"
	fi
    fi
}

if [ '@abs_top_srcdir@' = '' ] ; then
  echo "Something is wrong abs_top_srcdir is not set."
 exit 1
fi
abs_top_srcdir=@abs_top_srcdir@

# Make sure @abs_top_srcdir@ has a trailing slash
abs_top_srcdir=${abs_top_srcdir%%/}/

. $abs_top_srcdir/lib/tty.sh

set -o shwordsplit
SHUNIT_PARENT=$0

# load shunit2
shunit_file=${abs_top_srcdir}test/unit/shunit2
if [[ 0 == ${#funcfiletrace[@]} ]] ; then
    . ${shunit_file}
fi
